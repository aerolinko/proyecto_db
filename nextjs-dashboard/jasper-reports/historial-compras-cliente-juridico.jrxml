<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports
              http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="HistorialComprasClienteJuridico" pageWidth="595" pageHeight="842" columnWidth="555"
              leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20"
              uuid="g7h8i9j0-k1l2-3456-ghij-789012345678">
    
    <property name="com.jaspersoft.studio.data.defaultdataadapter" value="JSON Data Adapter"/>
    
    <parameter name="reportTitle" class="java.lang.String"/>
    <parameter name="generatedAt" class="java.lang.String"/>
    <parameter name="totalRecords" class="java.lang.Integer"/>
    <parameter name="clienteId" class="java.lang.String"/>
    <parameter name="fechaInicio" class="java.lang.String"/>
    <parameter name="fechaFin" class="java.lang.String"/>
    
    <field name="venta_id" class="java.lang.Integer"/>
    <field name="fecha_venta" class="java.lang.String"/>
    <field name="monto_total" class="java.lang.Double"/>
    <field name="razon_social" class="java.lang.String"/>
    <field name="rif" class="java.lang.String"/>
    <field name="producto_nombre" class="java.lang.String"/>
    <field name="cantidad" class="java.lang.Integer"/>
    <field name="precio_unitario" class="java.lang.Double"/>
    <field name="subtotal_producto" class="java.lang.Double"/>
    <field name="metodo_pago" class="java.lang.String"/>
    <field name="empleado_atendio" class="java.lang.String"/>
    
    <title>
        <band height="120" splitType="Stretch">
            <textField>
                <reportElement x="0" y="0" width="555" height="30" uuid="title-field"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="16" isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA[$P{reportTitle}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="0" y="40" width="555" height="20" uuid="date-field"/>
                <textElement textAlignment="Center">
                    <font size="10"/>
                </textElement>
                <textFieldExpression><![CDATA["Generado el: " + $P{generatedAt}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="0" y="60" width="555" height="20" uuid="filter-field"/>
                <textElement textAlignment="Center">
                    <font size="10"/>
                </textElement>
                <textFieldExpression><![CDATA["Cliente ID: " + $P{clienteId} + " | Período: " + ($P{fechaInicio} != null ? $P{fechaInicio} : "Sin fecha inicio") + " - " + ($P{fechaFin} != null ? $P{fechaFin} : "Sin fecha fin")]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="0" y="80" width="555" height="20" uuid="description-field"/>
                <textElement textAlignment="Center">
                    <font size="10" isItalic="true"/>
                </textElement>
                <textFieldExpression><![CDATA["Resumen detallado de todas las compras realizadas por razón social específica"]]></textFieldExpression>
            </textField>
        </band>
    </title>
    
    <columnHeader>
        <band height="30" splitType="Stretch">
            <staticText>
                <reportElement x="0" y="0" width="40" height="30" uuid="header-id"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Venta]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="40" y="0" width="70" height="30" uuid="header-date"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Fecha]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="110" y="0" width="80" height="30" uuid="header-product"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Producto]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="190" y="0" width="40" height="30" uuid="header-qty"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Cant]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="230" y="0" width="60" height="30" uuid="header-price"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Precio]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="290" y="0" width="70" height="30" uuid="header-subtotal"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Subtotal]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="360" y="0" width="80" height="30" uuid="header-payment"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Pago]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="440" y="0" width="115" height="30" uuid="header-employee"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Empleado]]></text>
            </staticText>
        </band>
    </columnHeader>
    
    <detail>
        <band height="20" splitType="Stretch">
            <textField>
                <reportElement x="0" y="0" width="40" height="20" uuid="detail-id"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{venta_id}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="40" y="0" width="70" height="20" uuid="detail-date"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{fecha_venta}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="110" y="0" width="80" height="20" uuid="detail-product"/>
                <textElement textAlignment="Left" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{producto_nombre}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="190" y="0" width="40" height="20" uuid="detail-qty"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{cantidad}]]></textFieldExpression>
            </textField>
            
            <textField pattern="#,##0.00">
                <reportElement x="230" y="0" width="60" height="20" uuid="detail-price"/>
                <textElement textAlignment="Right" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{precio_unitario}]]></textFieldExpression>
            </textField>
            
            <textField pattern="#,##0.00">
                <reportElement x="290" y="0" width="70" height="20" uuid="detail-subtotal"/>
                <textElement textAlignment="Right" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{subtotal_producto}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="360" y="0" width="80" height="20" uuid="detail-payment"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{metodo_pago}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="440" y="0" width="115" height="20" uuid="detail-employee"/>
                <textElement textAlignment="Left" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{empleado_atendio}]]></textFieldExpression>
            </textField>
        </band>
    </detail>
    
    <summary>
        <band height="80" splitType="Stretch">
            <textField>
                <reportElement x="0" y="0" width="555" height="20" uuid="summary-total"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="12" isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA["Total de registros de compra: " + $P{totalRecords}]]></textFieldExpression>
            </textField>
            
            <textField pattern="#,##0">
                <reportElement x="0" y="20" width="555" height="20" uuid="summary-items"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="12" isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA["Total de productos comprados: " + $V{SUM_cantidad}]]></textFieldExpression>
            </textField>
            
            <textField pattern="#,##0.00">
                <reportElement x="0" y="40" width="555" height="20" uuid="summary-subtotal"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="12" isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA["Subtotal de productos: $" + $V{SUM_subtotal_producto}]]></textFieldExpression>
            </textField>
            
            <textField pattern="#,##0.00">
                <reportElement x="0" y="60" width="555" height="20" uuid="summary-total-amount"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="12" isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA["Monto total de ventas: $" + $V{SUM_monto_total}]]></textFieldExpression>
            </textField>
        </band>
    </summary>
    
    <variable name="SUM_cantidad" class="java.lang.Integer" calculation="Sum">
        <variableExpression><![CDATA[$F{cantidad}]]></variableExpression>
    </variable>
    
    <variable name="SUM_subtotal_producto" class="java.lang.Double" calculation="Sum">
        <variableExpression><![CDATA[$F{subtotal_producto}]]></variableExpression>
    </variable>
    
    <variable name="SUM_monto_total" class="java.lang.Double" calculation="Sum">
        <variableExpression><![CDATA[$F{monto_total}]]></variableExpression>
    </variable>
</jasperReport> 