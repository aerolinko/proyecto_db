<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports
              http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="ReposicionAnaqueles" pageWidth="595" pageHeight="842" columnWidth="555"
              leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20"
              uuid="d4e5f6g7-h8i9-0123-defg-456789012345">
    
    <property name="com.jaspersoft.studio.data.defaultdataadapter" value="JSON Data Adapter"/>
    
    <parameter name="reportTitle" class="java.lang.String"/>
    <parameter name="generatedAt" class="java.lang.String"/>
    <parameter name="totalRecords" class="java.lang.Integer"/>
    <parameter name="fechaInicio" class="java.lang.String"/>
    <parameter name="fechaFin" class="java.lang.String"/>
    
    <field name="reposicion_id" class="java.lang.Integer"/>
    <field name="fecha_reposicion" class="java.lang.String"/>
    <field name="cantidad_solicitada" class="java.lang.Integer"/>
    <field name="estado" class="java.lang.String"/>
    <field name="producto_nombre" class="java.lang.String"/>
    <field name="stock_actual" class="java.lang.Integer"/>
    <field name="numero_pasillo" class="java.lang.String"/>
    <field name="zona" class="java.lang.String"/>
    <field name="lugar_nombre" class="java.lang.String"/>
    <field name="empleado_solicitante" class="java.lang.String"/>
    
    <title>
        <band height="120" splitType="Stretch">
            <textField>
                <reportElement x="0" y="0" width="555" height="30" uuid="title-field"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="16" isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA[$P{reportTitle}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="0" y="40" width="555" height="20" uuid="date-field"/>
                <textElement textAlignment="Center">
                    <font size="10"/>
                </textElement>
                <textFieldExpression><![CDATA["Generado el: " + $P{generatedAt}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="0" y="60" width="555" height="20" uuid="filter-field"/>
                <textElement textAlignment="Center">
                    <font size="10"/>
                </textElement>
                <textFieldExpression><![CDATA["Período: " + ($P{fechaInicio} != null ? $P{fechaInicio} : "Sin fecha inicio") + " - " + ($P{fechaFin} != null ? $P{fechaFin} : "Sin fecha fin")]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="0" y="80" width="555" height="20" uuid="description-field"/>
                <textElement textAlignment="Center">
                    <font size="10" isItalic="true"/>
                </textElement>
                <textFieldExpression><![CDATA["Órdenes automáticas cuando el stock desciende a 20 unidades"]]></textFieldExpression>
            </textField>
        </band>
    </title>
    
    <columnHeader>
        <band height="30" splitType="Stretch">
            <staticText>
                <reportElement x="0" y="0" width="40" height="30" uuid="header-id"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[ID]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="40" y="0" width="70" height="30" uuid="header-date"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Fecha]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="110" y="0" width="80" height="30" uuid="header-product"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Producto]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="190" y="0" width="50" height="30" uuid="header-stock"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Stock]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="240" y="0" width="50" height="30" uuid="header-qty"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Cant.]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="290" y="0" width="50" height="30" uuid="header-aisle"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Pasillo]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="340" y="0" width="50" height="30" uuid="header-zone"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Zona]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="390" y="0" width="80" height="30" uuid="header-location"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Ubicación]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="470" y="0" width="85" height="30" uuid="header-employee"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Empleado]]></text>
            </staticText>
        </band>
    </columnHeader>
    
    <detail>
        <band height="20" splitType="Stretch">
            <textField>
                <reportElement x="0" y="0" width="40" height="20" uuid="detail-id"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{reposicion_id}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="40" y="0" width="70" height="20" uuid="detail-date"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{fecha_reposicion}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="110" y="0" width="80" height="20" uuid="detail-product"/>
                <textElement textAlignment="Left" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{producto_nombre}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="190" y="0" width="50" height="20" uuid="detail-stock"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{stock_actual}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="240" y="0" width="50" height="20" uuid="detail-qty"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{cantidad_solicitada}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="290" y="0" width="50" height="20" uuid="detail-aisle"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{numero_pasillo}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="340" y="0" width="50" height="20" uuid="detail-zone"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{zona}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="390" y="0" width="80" height="20" uuid="detail-location"/>
                <textElement textAlignment="Left" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{lugar_nombre}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="470" y="0" width="85" height="20" uuid="detail-employee"/>
                <textElement textAlignment="Left" verticalAlignment="Middle">
                    <font size="7"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{empleado_solicitante}]]></textFieldExpression>
            </textField>
        </band>
    </detail>
    
    <summary>
        <band height="60" splitType="Stretch">
            <textField>
                <reportElement x="0" y="0" width="555" height="20" uuid="summary-total"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="12" isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA["Total de reposiciones generadas: " + $P{totalRecords}]]></textFieldExpression>
            </textField>
            
            <textField pattern="#,##0">
                <reportElement x="0" y="20" width="555" height="20" uuid="summary-qty"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="12" isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA["Total de unidades solicitadas: " + $V{SUM_cantidad_solicitada}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="0" y="40" width="555" height="20" uuid="summary-status"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="12" isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA["Productos con stock crítico (≤20 unidades): " + $V{COUNT_stock_critico}]]></textFieldExpression>
            </textField>
        </band>
    </summary>
    
    <variable name="SUM_cantidad_solicitada" class="java.lang.Integer" calculation="Sum">
        <variableExpression><![CDATA[$F{cantidad_solicitada}]]></variableExpression>
    </variable>
    
    <variable name="COUNT_stock_critico" class="java.lang.Integer" calculation="Count">
        <variableExpression><![CDATA[$F{stock_actual} <= 20 ? 1 : 0]]></variableExpression>
    </variable>
</jasperReport> 