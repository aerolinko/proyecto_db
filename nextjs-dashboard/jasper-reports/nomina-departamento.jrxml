    <variable name="SUM_salario_base" class="java.lang.Double" calculation="Sum">
        <variableExpression><![CDATA[$F{salario_base}]]></variableExpression>
    </variable>
    
    <variable name="SUM_beneficios" class="java.lang.Double" calculation="Sum">
        <variableExpression><![CDATA[$F{beneficios}]]></variableExpression>
    </variable>
    
    <variable name="SUM_costo_total" class="java.lang.Double" calculation="Sum">
        <variableExpression><![CDATA[$F{costo_total}]]></variableExpression>
