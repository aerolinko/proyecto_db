            <staticText>
                <reportElement x="410" y="0" width="60" height="30" uuid="header-benefits"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="8" isBold="true"/>
                </textElement>
                <text><![CDATA[Beneficios]]></text>
            </staticText>
            
            <staticText>
                <reportElement x="470" y="0" width="85" height="30" uuid="header-total"/>
