            <textField>
                <reportElement x="0" y="0" width="555" height="20" uuid="summary-total"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="12" isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA["Total de empleados: " + $P{totalRecords}]]></textFieldExpression>
            </textField>
            
            <textField pattern="#,##0.00">
                <reportElement x="0" y="20" width="555" height="20" uuid="summary-total-salary"/>
