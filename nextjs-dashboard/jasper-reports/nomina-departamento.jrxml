    <title>
        <band height="120" splitType="Stretch">
            <textField>
                <reportElement x="0" y="0" width="555" height="30" uuid="title-field"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="16" isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA[$P{reportTitle}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="0" y="40" width="555" height="20" uuid="date-field"/>
                <textElement textAlignment="Center">
                    <font size="10"/>
                </textElement>
                <textFieldExpression><![CDATA["Generado el: " + $P{generatedAt}]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="0" y="60" width="555" height="20" uuid="filter-field"/>
                <textElement textAlignment="Center">
                    <font size="10"/>
                </textElement>
                <textFieldExpression><![CDATA["Período: " + ($P{fechaInicio} != null ? $P{fechaInicio} : "Sin fecha inicio") + " - " + ($P{fechaFin} != null ? $P{fechaFin} : "Sin fecha fin")]]></textFieldExpression>
            </textField>
            
            <textField>
                <reportElement x="0" y="80" width="555" height="20" uuid="description-field"/>
                <textElement textAlignment="Center">
                    <font size="10" isItalic="true"/>
                </textElement>
                <textFieldExpression><![CDATA["Salario y beneficios asociados a cada empleado por cargo y departamento"]]></textFieldExpression>
            </textField>
        </band>
